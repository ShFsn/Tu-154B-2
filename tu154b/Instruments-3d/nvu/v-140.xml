<!-- V-140 NVU (navigation system) control
3D instrument for Tu-154 -->
<PropertyList>
 <path>v-140.ac</path>
<!--External panel lighting-->
<animation>
  <type>material</type>
  <object-name>base</object-name>
  <emission>
    <red-prop>sim/model/cabin-lighting/vnesh</red-prop>
    <green-prop>sim/model/cabin-lighting/vnesh</green-prop>
    <blue-prop>sim/model/cabin-lighting/vnesh</blue-prop>
  </emission>
</animation>

<!-- Internal instruments lighting -->
<animation>
  <type>material</type>
  <object-name>text</object-name>
  <object-name>digits</object-name>
  <emission>
    <red-prop>sim/model/cabin-lighting/lamps</red-prop>
    <green-prop>sim/model/cabin-lighting/lamps</green-prop>
    <blue-prop>sim/model/cabin-lighting/lamps-2</blue-prop>
  </emission>
</animation>
<animation>
  <type>material</type>
  <object-name>base_b</object-name>
  <emission>
    <red-prop>sim/model/cabin-lighting/lamps</red-prop>
    <green-prop>sim/model/cabin-lighting/lamps</green-prop>
    <blue-prop>sim/model/cabin-lighting/lamps</blue-prop>
  </emission>
</animation>

<!--Indicators-->
<animation>
  <type>material</type>
  <object-name>I</object-name>
  <object-name>II</object-name>
  <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
    <factor-prop>tu154/systems/electrical/checking-lamps/lamps-power</factor-prop>
  </emission>
</animation>

<animation>
  <type>select</type>
  <object-name>I</object-name>
  <condition>
    <equals>
      <property>fdm/jsbsim/instrumentation/nvu/active</property>
      <value>1</value>
    </equals>
  </condition>
</animation>
<animation>
  <type>select</type>
  <object-name>II</object-name>
  <condition>
    <equals>
      <property>fdm/jsbsim/instrumentation/nvu/active</property>
      <value>2</value>
    </equals>
  </condition>
</animation>
