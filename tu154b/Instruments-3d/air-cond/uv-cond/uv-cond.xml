      <entry><ind>  3000.0</ind> <dep> 33.0</dep></entry>
      <entry><ind>  3500.0</ind> <dep> 63.0</dep></entry>
      <entry><ind>  4000.0</ind> <dep> 89.5</dep></entry>
      <entry><ind>  4500.0</ind> <dep> 115.5</dep></entry>
