<?xml version="1.0"?>
<PropertyList>
  <path>switch.ac</path>
  <params>
    <switch/>
  </params>

  <animation>
    <type>rotate</type>
    <object-name>handle</object-name>
    <property alias="../../params/switch"/>
    <factor>-120</factor>
    <offset-deg>60</offset-deg>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>hotspot_1</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-interpolate</command>
        <property alias="../../../../params/switch"/>
        <value>1</value>
        <time>0.1</time>
      </binding>
    </action>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>hotspot_2</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-interpolate</command>
        <property alias="../../../../params/switch"/>
        <value>0</value>
        <time>0.1</time>
      </binding>
    </action>
  </animation>
</PropertyList>
