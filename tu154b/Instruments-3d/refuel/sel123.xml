<?xml version="1.0"?>
<PropertyList>
  <path>handle.ac</path>

  <animation>
    <type>knob</type>
    <object-name>handle</object-name>
    <property>fdm/jsbsim/fuel/sel-refuel-123</property>
    <visible>true</visible>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/fuel/sel-refuel-123</property>
        <min>0</min>
        <max>3</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <interpolation>
      <entry><ind>0</ind><dep>-47</dep></entry>
      <entry><ind>3</ind><dep>45</dep></entry>
    </interpolation>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>
</PropertyList>
