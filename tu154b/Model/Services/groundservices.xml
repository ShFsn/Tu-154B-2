<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>
	<name>groundservices</name>
	
	<!-- Fuel Truck -->
	
	<model>
		<name>fuel-truck-l</name>
		<path>Aircraft/tu154b/Model/Services/Fuel-Truck/Fuel_Truck_Short_VolvoFM.ac</path>
		<condition>
			<and>
				<property>sim/model/ground-services/fuel-l</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>-14.0</x-m>
			<y-m>-28.0</y-m>
			<z-m>2.8</z-m>
			<heading-deg>105</heading-deg>
		</offsets>
	</model>
	<model>
		<name>fuel-truck-r</name>
		<path>Aircraft/tu154b/Model/Services/Fuel-Truck/Fuel_Truck_Short_VolvoFM.ac</path>
		<condition>
			<and>
				<property>sim/model/ground-services/fuel-r</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>8.5</x-m>
			<y-m>12.5</y-m>
			<z-m>-2.1</z-m>
			<heading-deg>245</heading-deg>
		</offsets>
	</model>
	
	<animation>
		<type>select</type>
		<object-name>fuel_pipe</object-name>
		<condition>
			<and>
				<property>sim/model/ground-services/f-connect-r</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>fuel_pipe_disconnected</object-name>
		<condition>
			<and>
				<not><property>sim/model/ground-services/f-connect-r</property></not>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
	</animation>
	
	<!-- De-icing Truck -->
	
	<model>
		<name>de-ice-truck</name>
		<path>Aircraft/tu154b/Model/Services/De-Icing/volvo_deicing_truck.xml</path>
		<condition>
			<and>
				<property>sim/model/ground-services/de-ice</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>33</x-m>
			<y-m>10.0</y-m>
			<z-m>-1.6</z-m>
			<heading-deg>20</heading-deg>
			<pitch-deg>-1</pitch-deg>
		</offsets>
	</model>
	
	<!-- Catering Truck - 1st door -->
	
	<model>
		<name>cater-truck</name>
		<path>Aircraft/tu154b/Model/Services/Catering/L_T_VolvoFM-4_5m-set.xml</path>
		<condition>
			<and>
				<property>sim/model/ground-services/catering</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>-8.75</x-m>
			<y-m>6.62</y-m>
			<z-m>-2.4</z-m>
			<heading-deg>90.0</heading-deg>
		</offsets>
	</model>
	
	<!-- Baggage Truck -->
	
	<model>
		<name>baggage-truck-front</name>
		<path>Aircraft/tu154b/Model/Services/Baggage/cargo_ramp.ac</path>
		<condition>
			<and>
				<property>sim/model/ground-services/f-buggage</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>-27.6</x-m>
			<y-m>-0.4</y-m>
			<z-m>2.4</z-m>            
		</offsets>
	</model>
	<model>
		<name>baggage-truck-back</name>
		<path>Aircraft/tu154b/Model/Services/Baggage/cargo_ramp.ac</path>
		<condition>
			<and>
				<property>sim/model/ground-services/r-buggage</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>1.6</x-m>
			<y-m>18.5</y-m>
			<z-m>2.6</z-m> 
			<heading-deg>270</heading-deg>          
		</offsets>
	</model>
	
	<!-- External Power -->
	
	<model>
		<name>ext-pwr</name>
		<path>Aircraft/tu154b/Model/Services/External-Power/ext-pwr-box.ac</path>
		<condition>
			<and>
				<property>sim/model/ground-services/ext-power</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>-16.55</x-m>
			<y-m>2.95</y-m>
			<z-m>2.15</z-m>
			<heading-deg>300</heading-deg>
		</offsets>
	</model>
	
	<!-- Chocks -->
	
	<model>
		<name>chock_nose</name>
		<path>Aircraft/tu154b/Model/Services/Chocks/chock_nose.ac</path>
		<condition>
			<and>
				<property>sim/model/ground-services/chocks</property>
