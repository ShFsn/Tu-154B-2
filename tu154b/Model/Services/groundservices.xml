					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>-1.8</x-m>
			<y-m>0.02</y-m>
			<z-m>2.75</z-m>
		</offsets>
	</model>
	
	<model>
		<name>chock_left</name>
		<path>Aircraft/tu154b/Model/Services/Chocks/chock_nose.ac</path>
		<condition>
			<and>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>15.85</x-m>
			<y-m>-5.75</y-m>
			<z-m>3.0</z-m>
		</offsets>
	</model>
	
	<model>
		<name>chock_right</name>
		<path>Aircraft/tu154b/Model/Services/Chocks/chock_nose.ac</path>
		<condition>
			<and>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
		<offsets>
			<x-m>15.85</x-m>
			<y-m>5.8</y-m>
			<z-m>3.0</z-m>
		</offsets>
	</model>
	<!-- Stairways -->
	

	<model>
		<path>Aircraft/tu154b/Model/Services/Stairs/stairs.xml</path>
		<name>l1</name>
		<offsets>
			<x-m>-7.7</x-m>
			<y-m>-0.2</y-m>
			<z-m>1.1</z-m>
			<heading-deg>0.0</heading-deg>
		</offsets>
		<condition>
			<and>
				<property>sim/model/ground-services/stairs</property>
				<property>sim/model/ground-services/chocks</property>
				<less-than>
					<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
	</model>

